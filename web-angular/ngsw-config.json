{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "i18n",
      "urls": [
        "https://me.guillaumetalbot.com/applicationBlancheRest/i18n/*"
      ],
      "cacheConfig": {
        "maxAge": "30d",
        "strategy": "freshness"
      }
    }
  ]
}