{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/images/*.jpg"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "i18n",
      "urls": [
        "/*/i18n/*"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "30d",
        "strategy": "freshness"
      }
    }
  ]
}