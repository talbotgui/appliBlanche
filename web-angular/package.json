{
  "name": "web-angular",
  "version": "1.0.0",
  "description": "Application WEB de l'application blanche avec Angular",
  "author": "Guillaume TALBOT <talbotgui@gmail.com>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=8181",
    "build-prod": "ng build --prod --aot --progress=false --verbose=true --build-optimizer --base-href /applicationBlanche/ --output-path ./dist/unix/applicationBlanche",
    "start-prod": "http-server -p 8282 -c-1 dist/unix/",
    "build-gcloud": "ng build --prod --aot --progress=false --verbose=true --build-optimizer --base-href / --output-path ./dist/gcloud/applicationBlanche",
    "quality": "node node_modules/sonar-scanner/ -Dproject.settings=./sonar-applicationBlancheAngular.properties",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "debug-e2e": "ng e2e --specs=e2e/unTestEnParticulier.e2e-spec.ts --serve=false",
    "e2e-ic": "ng e2e --protractor-config=protractor-ic.conf.js",
    "doc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d ./target/documentation",
    "quality-doc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json --coverageMinimumPerFile 50"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.4",
    "@angular/cdk": "7.0.4",
    "@angular/common": "7.0.4",
    "@angular/compiler": "7.0.4",
    "@angular/core": "7.0.4",
    "@angular/forms": "7.0.4",
    "@angular/http": "7.0.4",
    "@angular/material": "7.0.4",
    "@angular/platform-browser": "7.0.4",
    "@angular/platform-browser-dynamic": "7.0.4",
    "@angular/platform-server": "7.0.4",
    "@angular/pwa": "0.10.6",
    "@angular/router": "7.0.4",
    "@angular/service-worker": "7.0.4",
    "@fortawesome/fontawesome-free": "5.6.0",
    "@stomp/ng2-stompjs": "^7.0.1",
    "@types/html2canvas": "0.0.35",
    "@types/jspdf": "1.2.2",
    "angular-l10n": "5.0.0",
    "bootstrap": "4.1.1",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "html2canvas": "^1.0.0-alpha.12",
    "jspdf": "1.4.1",
    "ng-pwa-tools": "0.0.15",
    "ngx-bootstrap": "3.0.1",
    "popper.js": "1.14.3",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.0",
    "@angular/cli": "7.0.5",
    "@angular/compiler-cli": "7.0.4",
    "@angular/language-service": "7.0.4",
    "@compodoc/compodoc": "1.1.5",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.4",
    "@types/node": "8.9.4",
    "codelyzer": "4.5.0",
    "http-server": "0.11.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.2",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-junit-reporter": "1.2.0",
    "protractor": "5.4.1",
    "sonar-scanner": "3.0.3",
    "ts-mockito": "2.3.1",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "~3.1.1"
  }
}
